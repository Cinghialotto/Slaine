				JMPTL	DB >MOVE	; 1	DB >EXAMINE	; 2	DB >LOOK	; 3	DB >STATUS	; 4	DB >OBJECTS	; 5	DB >ACTIONS	; 6	DB >HITUKKO	; 7	DB >INVEN	; 8	DB >ATTACK	; 9	DB >OK		; 10	DB >YES,>NO	; 11 TO 12	DB >OVER	; 13	DB >MN,>MNE,>ME,>MSE,>MS,>MSW,>MW,>MNW	; 14 TO 21	DB >PICKUP,>DROP,>TAKE,>PUTIN,>PUTON	; 22 TO 26	DB >OPEN,>CLOSE,>GIVETO,>TAKE,>EAT	; 27 TO 36	DB >ASK,>USE,>REST,>DRINKA,>GIVEUP	DB >AROUND,>LOOKIT,>LOOKON		; 37 TO 39	DB >WRP,>KIK,>DUK,>PNCH,>SWN	DB >LEP,>BEHD,>DEFN,>DODG,>THRO,>SHTJMPTH	DB <MOVE	DB <EXAMINE	DB <LOOK	DB <STATUS	DB <OBJECTS	DB <ACTIONS	DB <HITUKKO	DB <INVEN	DB <ATTACK	DB <OK	DB <YES,<NO	DB <OVER	DB <MN,<MNE,<ME,<MSE,<MS,<MSW,<MW,<MNW	; MOVE	DB <PICKUP,<DROP,<TAKE,<PUTIN,<PUTON	; OBJECTS	DB <OPEN,<CLOSE,<GIVETO,<TAKE,<EAT	; ACTIONS	DB <ASK,<USE,<REST,<DRINKA,<GIVEUP	DB <AROUND,<LOOKIT,<LOOKON		; LOOK	DB <WRP,<KIK,<DUK,<PNCH,<SWN	DB <LEP,<BEHD,<DEFN,<DODG,<THRO,<SHTITMTL	DB >EXAM,>PIK,>DRP,>TAK,>TAK2	DB >PUTN,>PUTN2	DB >PUTO,>OPN,>CLO,>GVTO	DB >ET,>US,>US2,>LOK,>LOKNITMTH	DB <EXAM,<PIK,<DRP,<TAK,<TAK2		DB <PUTN,<PUTN2	DB <PUTO,<OPN,<CLO,<GVTO	DB <ET,<US,<US2,<LOK,<LOKN	; ************* 1 *************MOVE	LDY #0		; MAKE NEW LIST	STY LISTO	LDX #0		MKLI2	LDA (MAPD),Y	; OPTNUMS	BEQ PLEE	; THAT CAN       TYA		; BE USED	CLC	ADC #14		; DIRECTIONS       STA LIST+0,X	INXPLEE	INY	CPY #8		; ALL OF THEM	BNE MKLI2ADDEN	LDA #10		; ADD AN OK	STA LIST+0,X	LDA #255	; END MARKER	STA LIST+1,XFANG	RTS	; ************* 2 *************EXAMINE LDA #0		; EXAMINE OBJECT	STA DOWHAT	; WHICH OBJECT	JMP SLARO	; SLAINE & ROOMEXAM	JSR FACE	; CLEAR BOXES	LDY WHATP	; POINTER	LDX OBJ1,Y	; FROM OBJECT	JSR WINDOW	LDA WHATO	; START AT 1	CLC	ADC #67		; DISCRIPTION	TAX		; START AT 68	JSR WINDOW	LDA WHATO 	CMP #42	BNE FANG	LDA R78+2	; ALREADY	BNE FANG	; THERE	LDX #T+79	JSR WINDOW	LDA #79		; OPEN MOUND	STA R78+2	RTS		; ************* 3 *************LOOK	LDX #2		CLCSETR1	TXA	ADC #37	STA LIST,X	DEX	BPL SETR1	LDX #3	JMP ADDEN	; ************* 4 *************STATUS	JSR FACE	LDX #4	JSR WINDOW	LDA CIA2+11	; HOURS	BIT CIA2+8	; START AGAIN	CMP #1		; IF 1 HOUR +4	BEQ XST	LDX #0		; FROM 0 TO 3	NOPPXST	LDX #4		; FROM 4 TO 7	LDA CIA2+10	CMP #&15	BCC THS	INX		; +1	CMP #&30			BCC THS	INX		; +2	CMP #&45	BCC THS	INX		; +3THS	TXA	CLC	ADC #T+26	TAX	JSR WINDOW	JSR WRR	JSR WINDOW	LDX #T+25	JSR WINDOW	JSR MONY	JMP WINDOWMONY	LDA MONEY	LSR A		LSR A	LSR A	LSR A	ORA #"0"	STA STEXB2+9	LDA MONEY	AND #&F	ORA #"0"		STA STEXB2+10	LDX #T+46	RTSWRR	LDA WARP+1	; HIGH BIT	AND #&F		ORA #"0"	STA STEX90+17	LDA WARP+0	; LOW BIT	LSR A	LSR A	LSR A	LSR A	ORA #"0"	STA STEX90+18 	LDA WARP+0	AND #&F		ORA #"0"	STA STEX90+19	LDA MAXW+1	; HIGH BIT	AND #&F	ORA #"0"	STA STEX91+25	LDA MAXW+0	; LOW BIT	LSR A		LSR A	LSR A	LSR A	ORA #"0"	STA STEX91+26	LDA MAXW+0	AND #&F	ORA #"0"	STA STEX91+27	LDX #T+24	; WAPR RATINGRET2	RTS	; ************* 5 *************OBJECTS LDX #4	CLCSETR2	TXA	ADC #22		; STARTS AT 20	STA LIST,X	DEX	BPL SETR2	LDX #5	JMP ADDEN	; +END AND OK	; *************	6 *************ACTIONS LDX #7	CLCSETR3	TXA		ADC #27	STA LIST,X	DEX		BPL SETR3	LDX #8	JSR ADDEN	LDA #99	JSR GAIN	LDA #100	JSR GAIN	LDA ENEMYT+2	; IF NO ONE	BNE RET2	; IN THIS ROOM	LDA #32		; TAKE OUT	JMP LOOSE	; ASK	; ************* 7 *************HITUKKO	LDA ROOM	CMP #110	BEQ SPIC	LDA HITUKO	CLC		ADC #1	CMP #4	BCS TUR	STA HITUKO	LDY WHATY	; NOW FREE FOR	LDA #255	; A NEW SCROLL	STA OPTNUM,Y	JSR FACE	; CLEAR BOXES	LDA HITUKO	CMP #2	BEQ SECT	CMP #3	BEQ LAST	LDX #6	JSR WINDOW	LDX #T+0	; TEXT 0	JSR WINDOW	LDX #4	JSR WINDOW	LDX #T+3	; TEXT 3	JSR WINDOW	JMP STCLSECT	LDX #6	JSR WINDOW 	LDX #T+1	; TEXT 1	JSR WINDOW		JMP STCLLAST	LDX #6	JSR WINDOW	LDA #239	; NULL LOCATION	STA OBJ25+1	LDX #T+2	; TEXT 2	JSR WINDOW	LDX #17	JSR WINDOW	LDA #7		; DELETE FROM	JSR LOOSE	; LISTSTCL	LDA #0		; START CLOCK	STA CIA1+10	STA CIA1+9	STA CIA1+8TUR	RTS	SPIC	JSR FACE	LDA OBJ21+1	CMP #246	BEQ MIST	LDX #T+61	JMP WINDOWMIST	LDX #T+62	JSR WINDOW	LDA ROOM	STA DEMON	LDA #239	; DESTROY GEM	STA OBJ21+1	LDA #255	; PART 1	STA MAIN1	; COMPLETE	JMP DEMM	; *************	8 *************INVEN	JSR FACE	; CLEAR BOXES	LDX #5		; SLAINE SMILE	JSR WINDOW	LDA #0		; IF ON SLAINESHOWC	STA WHOME+1	; OR IN A	LDA #<OBJ1	; CONTAINER	STA TEMPM+1	;	LDA #>OBJ1	STA TEMPM+0	LDA #0		; TO SEE IF	STA WHATS	; PRINT ANYTHINGGOT	LDY #0	LDA (TEMPM),Y	; CHECK FOR END	CMP #255	; IF NOT	BEQ ENDOS	; X = PICTURE	TAX		; SAVES RE_LOAD	INY			LDA (TEMPM),Y	; HAS ITEMWHOME	CMP #255	; SMC	BNE ELSW	; IS SO THEN	LDA #255	; TELL IF	STA WHATS	; GOT ANYTHING	JSR WINDOW	; SHOW ITEMELSW	LDA TEMPM+0	CLC	ADC #4	STA TEMPM+0	; GET NEXT	BCC GOT		; OBJECT MEM	INC TEMPM+1	BNE GOT		; ALLWAYSENDOS	RTS		; END OF SEARCH	; ************* 9 *************	BLOCKATTACK	JSR FACE	JSR CLEARB	JSR FADEO	JSR ATTK	LDX #>K0	LDY #<K0	JSR WHATIW	LDA #239	STA FIGHT	STA OBJ25+1	LDX #2		; SLAINE ANGRY	JSR WINDOW	JSR WRR		; YOUR WAPR	JSR WINDOW	JSR HOMV	; HOW MANY OF THEM	JSR WINDOW	JSR ENW	JSR WINDOW	LDA #41	STA LIST+0	LDA #43	STA LIST+1	LDA #45	STA LIST+2	LDA #255	STA LIST+3	LDA #0	STA LISTO	STA AXE		; HAVE GOT IT	LDA #5		; 4 SECONDS	STA TIMER	LDA MUSIC	BMI NOTUN	LDA #1	JMP &5700NOTUN	RTSENW	LDA ENEMYT+2	STA MIKE1	SED	LDA #0	STA ENEMYW:LOOP	LDA ENEMYW	CLC	ADC ENEMYT+3	STA ENEMYW	LDA MIKE1		SEC	SBC #1	STA MIKE1	BNE :LOOP	CLD	ENNW	LDA ENEMYW	LSR A	LSR A	LSR A	LSR A	ORA #"0"	STA STEXA7+18	LDA ENEMYW	AND #&F	ORA #"0"	STA STEXA7+19	LDX #T+41		RTSERROR	INC BORDER	RTS	; ************ 10 *************OK	LDX #9SETF	TXA	STA LIST-1,X	DEX	BNE SETF	LDA #255	STA LIST+9	LDA #0	STA LISTO	LDA OBJ25+1	; IF UKKO	BPL ENDOR	; NOT HERE	LDA #7		; EXCLUDE	JSR LOOSE	; FROM LISTENDOR	LDA ROOM	; IF IN	CMP #114	; BAR	BEQ LOATT	; NO ATTACK	LDA ENEMYT+2	BNE WREWLOATT	LDA #9		; NO ATTACK	JSR LOOSEWREW	JMP NORM	; SCROLL COLOUR	; ************ 11 *************YES	LDA YESB	BEQ ING	CMP #1	BEQ DRNKC	LDA MONEY	; ENOUGTH	CMP #&25	; MONEY ?	BCC NOLOT	SED	SBC #&25	; IF SO	STA MONEY	; TAKE FROM	CLD		; YOUR GOLD	LDX #T+73	; HE TAKES YOU	JSR WINDOW	; ACROSS RIVER	LDA #0	STA YESB	; BACK TO NORMAL	STA NOB	LDA #66		; BOAT MAN	STA BOAT+0	; SAME PLACE	JMP ANDDE	; AS YOUDRNKC	LDA MONEY	CMP STAKE	BCS YOURINOLOT	LDX #T+51	JSR WINDOW	JMP LACKYOURI	JSR FACE	LDX #T+49	JSR WINDOW	LDX #T+50	JSR WINDOW	LDA #35	STA LIST+0	LDA #36	STA LIST+1	LDA ELAPSE	STA TIMER2	LDA #0		; AMOUNT TO DRINK	STA VDRUNK	STA DRUNK	STA BODG+1	; IF HALF WAY	STA BODG2+1	; ON LAST ONE	LDA #255	; START NOW	STA DRINK	RTSING	JSR CLEARB	JSR FADEO	; FADE ALL	LDX #T+5	; SHALL BEGIN	JSR WINDOW	LDX #50	BLOCK:LOOP	LDA SYNCWAIT	CMP SYNC	BEQ WAIT	DEX	BNE :LOOP	JSR FACE	JSR OK	LDA #0	STA CIA2+8	; START CIA2		LDA #LOC	JMP ANDDE	; DISCRP. TEXT	; ************ 12 ************NO	LDA NOB			BEQ NOTHNLACK	LDA #0	STA NOB	STA YESB	JSR CLEARB	; BOTTLE OUT	JSR FADEO	; OF DRINK	JSR FACE	; CONTEST	JMP OK		; BACK TO MENUNOTHN	RTS	; ************ 13 ************OVER	JSR CLEARB	; END OF GAME	JSR FADEO	; FADE ALL		JMP RESTART	; RESTART	; ********* 14 TO 21 *********MN	LDY #0		; NORTH	NOPPMNE	LDY #1		; NORTH EAST	NOPPME	LDY #2		; EAST		NOPPMSE	LDY #3		; SOUTH EAST	NOPPMS	LDY #4		; SOUTH	NOPPMSW	LDY #5		; SOUTH WEST	NOPPMW	LDY #6		; WEST		NOPPMNW	LDY #7		; NORTH WEST	NOPPMUP	LDY #8		; UP	NOPPMDOWN	LDY #9		; DOWN	STY NEWL+1	; NEW LOCATION		LDA ROOM	CMP #82	BNE CANGO	CPY #1	BNE CANGO	LDA OBJ14+3		CMP #32	BEQ CANGO	JSR FACE	LDX #T+65	; NO LIT CANDLE	JMP WINDOWCANGO	JSR CLEARB	; STOP SCROLLS	JSR FADEO	; A	JSR FACE	LDA #2		; DRINK	STA STAKE	; CONTEST	LDA #150	; SET TIMER	STA ELAPSE	; FOR DRINKERNEWL	LDY #255	; SMC	LDA (MAPD),Y	; NEW ROOMANDDE	STA ROOM	; NUMBER	STA OBJ25+1	; UKKO AS WELL	TAY	LDA MAPPL,Y	; MAP DATA	STA MAPD+0	; POINTERS	LDA MAPPH,Y	; HI & LO 		STA MAPD+1	; BYTES	LDY #10		; 10th = PIC NO.	LDA (MAPD),Y	TAX		; PRINT IT	JSR FIRSTW	; GRAFIX OR TEXT	JSR PTEXT	; DISCRIPTIONDEMM	LDX #0	STX ENEMYT+2	; NO ONE HERE	STX KILL	; AND TO KILL	ANYOV	LDA ENEMYS,X	CMP #255	BEQ ESD	CMP ROOM	BEQ THEM	TXA	CLC	ADC #8	TAX	BNE ANYOVTHEM	STX VILOFF	LDY #0COPB	LDA ENEMYS,X	STA ENEMYT,Y	INX	INY	CPY #8	BNE COPB	JSR HOMV	JSR WINDOW	LDA ENEMYT+6	BEQ ESD	LDX #T+40	JSR WINDOW	LDA #5	STA TIMER	LDA #255	STA KILLESD	JMP OK		; NO ONE THEREHOMV	LDA ENEMYT+2	; HOW MANY	LSR A		; ENEMYS	LSR A	LSR A	LSR A	ORA #"0"	STA STEXA4+9	STA STEXA5+9	LDA ENEMYT+2	AND #&F	ORA #"0"	STA STEXA4+10	STA STEXA5+10	LDX ENEMYT+1	RTS	; ************ 22 **************PICKUP	LDA #1		; PIK	JMP INROOM	; IN ROOMPIK	LDA WHATS	; DELETE	JSR LOOSE	; LIST	JSR FACE	LDY WHATP	LDA OBJ1+2,Y	CMP #50	BEQ SILY	CMP #60	BEQ SILY2	LDA #0	STA OBJ1+1,Y	; NOW SLAINESSHOW	LDX OBJ1,Y	; SHOW WHAT GOT	JMP WINDOWSILY	LDX #T+7	; IF OBJECT TOO	NOPPSILY2	LDX #T+68	; CAN'T CARRY	JMP WINDOW	; BIG TO CARRY	; ************ 23 **************DROP	LDA #2		; DRP	JMP SLAIN	; FROM SLAINEDRP	JSR ITM	BCC DEFWQ	RTSDEFWQ	LDA WHATS	JSR LOOSE	LDY WHATP	LDA ROOM	; IN ROOM			STA OBJ1+1,Y		LDX OBJ1,Y	JMP WINDOW	; ************ 24 **************TAKE	LDA #3		; TAKE FROM	JMP SLARO	; WHAT OBJECT			TAK	JSR FACE	JSR CLEARB	JSR FADEO	LDY WHATO	; TAKE WHAT	STY TAK2+1	LDA CONT-1,Y	STA CONN+1	BNE YESC	JMP SLB		; SILLYYESC	LDY WHATP	LDA OBJ1+3,Y	AND #%00001000	; TAKE FROM	BEQ NOZT	; ON A OBJECT	LDA OBJ1+3,Y	; IF OBJECT	AND #%00000001	; CAN BE	BEQ NOZT	; OPENED	LDA OBJ1+3,Y	; CHECK IF SO	AND #%00000010	; ELSE	BNE NOZT	; IGNORECCL	LDX #T+9	; CONTAINER	JMP WINDOW	; CLOSEDNOZT	LDA WHATO	CMP #25	BNE NOUKO	LDA HITUKO	BEQ FOFF   	LDA CIA1+10	CMP #1	BCC OKAYYFOFF	LDX #T+34	; KEEP YOU HANDS	JSR WINDOW	JMP OK		; RESET OPTIONSOKAYY	LDX #T+35	JSR WINDOWNOUKO	LDA #4	STA DOWHAT	LDY #0		; MAKE LIST OF	LDX #0		; OBJECTSCRIO	LDA OBJ1,Y	; THAT ARE	CMP #255	; INSIDE	BEQ EXT		; CONTAINER	LDA OBJ1+1,YCONN	CMP #255	; CONT.NO.	BNE SODF		TYA		; Y DIV 4	LSR A		; =OBJECT NUMBER	LSR A	CLC	ADC #51		; OBJ START	STA LIST,X	INXSODF	INY	INY	INY	INY	BNE CRIOEXT	JSR ADDEN	; +OK AND END	LDX #T+13	; TAKE WHAT	JMP WINDOW	; OBJECT	TAK2	LDA #255	CMP #20	BNE NOPH	LDA WHATO	CMP #21	BNE NOPH	JSR FACE	LDX #T+58	JMP WINDOWNOPH	LDY WHATP	LDA #0		; NOW ON	STA OBJ1+1,Y	; SLAINE	LDA WHATS	; DELETE	JSR LOOSE	; FROM LIST	LDX OBJ1,Y	; SHOW GOT	JMP WINDOW	; OBJECT  	; ************ 25 **************PUTIN	LDA #5		; INTO A CONT.?	JMP SLARO	; ANYWAREPUTN	JSR FACE	JSR CLEARB	JSR FADEO	LDY WHATO	; SEE IF IT IS	LDA CONT-1,Y	; A CONTAINER	BEQ TBTN	; SILLY IF NOT	STA OBJT+1	; OBJECT NUMBER	LDY WHATP	LDA OBJ1+3,Y	; CAN NOT PUT	AND #%00001000	; INTO THIS	BNE TBTN	; UKKO BAR ETC	LDA OBJ1+3,Y	; IF CONTAINER	AND #%00000001	; CAN BE OPENED	BEQ SOTH	; THEN CHECK	LDA OBJ1+3,Y	; IF SO	AND #%00000010		BNE SOTH	; CLOSED	JMP CCLSOTH	LDA #6	STA DOWHAT	LDA WHATS	JSR LOOSE	LDX #T+15	; PUT WHAT	JMP WINDOW	PUTN2	JSR ITM	BCC CAR   	RTSCAR	LDY WHATP	LDA OBJ1+2,Y	CMP #50	BCC OBJTTBTN	JMP SLBOBJT	LDA #255	; SMC	CMP #249	BNE NOD3	LDX WHATO	CPX #22	BEQ DEMNNOD3	STA OBJ1+1,Y	LDA WHATS	JSR LOOSE	LDX OBJ1,Y	JMP WINDOWDEMN	LDA #239	STA OBJ22+1	LDA #255	STA MAIN3	LDA ROOM	STA DEMON	LDX #T+76	JSR WINDOW	JMP DEMM	; ************ 26 ************** 	PUTON	LDA #7	JMP SLAROPUTO	JSR FACE	JSR CLEARB	JSR FADEO	LDY WHATO	; SEE IF IT IS	LDA CONT-1,Y	; ACONTAINER	BEQ SLB		; SILLY IF NOT	STA OBJT+1	; OBJECT NUMBER	LDY WHATP	LDA OBJ1+3,Y	; IF CAN BE PUT	AND #%00001000	; ON	BEQ SLB	JMP SOTH	; ************ 27 **************	OPEN	LDA #8	JMP SLAROOPN	JSR FACE	LDA WHATO	CMP #44	BNE SWMH	LDX #T+74	JMP WINDOWSWMH	CMP #4	BEQ BOTL	LDY WHATP	LDA OBJ1+3,Y	STA TEMPM	AND #%00000001	BNE ITCANSLB	LDX #T+6	; SILLY	JMP WINDOWITCAN	LDA TEMPM	AND #%00000010	BEQ NOPN	; OPEN IT	LDX #T+8	; ALREADY OPEN	JMP WINDOWNOPN	LDA TEMPM	; NOW OPEN	ORA #%00000010	; IT	STA OBJ1+3,Y	JMP SHOW	; GRAFIX	BOTL	LDA OBJ4+1	; BOTTLE	STA OBJ19+1	; NEW BOTTLE	LDA #239	; DESTROY	STA OBJ4+1	; OLD BOTTLE	LDX #T+77	JSR WINDOW	LDA #255	STA MAIN4	LDA ROOM	STA DEMON	JMP DEMM	; ************ 28 **************CLOSE	LDA #9	JMP SLAROCLO	JSR FACE	LDY WHATP	LDA OBJ1+3,Y	STA TEMPM	AND #%00000001	BEQ SLB		; SILLY	LDA TEMPM	AND #%00000010	BNE SHUT	; CAN BE SHUT	LDX #T+9	; IT IS CLOSED	JMP WINDOWSHUT	LDA TEMPM	; CLOSE IT	AND #%11111101	STA OBJ1+3,Y	JMP SHOW	; ************ 29 **************GIVETO	LDA #10		; UKKO	JMP SLAIN	; ON YOUGVTO	JSR ITM	BCC TCRET	RTSTC	LDY WHATP	LDA #250	STA OBJ1+1,Y	LDX OBJ1+0,Y	JMP WINDOW	; ************ 31 **************EAT	LDA #11	JMP SLAIN	; ON YOUET	JSR FACE	LDY WHATP	LDA OBJ1+3,Y	AND #%00000100		BEQ SLB2	; SILLY		LDA #252	; DESTROY	STA OBJ1+1,Y	; OBJECT	LDX OBJ1,Y	; DISPLAY	JSR WINDOW	; FOOD	LDA WHATS	; SCROLL NUM.	JSR LOOSE	; LOOSE IT	LDX #T+11	JSR WINDOW		LDA WHATO	; IF COMSUME	CMP #27		; BEER THEN	BNE RET 	LDA #%11000000	; DRUNK	STA MOVEICON		LDX #T+23	; QUEZZY	JMP WINDOW			SLB2	LDX #T+10	; UURGGHH	JMP WINDOW		; ************ 32 **************ASK	LDA ROOM	CMP #114	BEQ CONTQ	CMP #35	BEQ BOTM	CMP #66	BEQ BACK	CMP #111	BEQ GIRL	RTSBACK	JSR FACE	LDX #T+73	; TAKES YOU	JSR WINDOW	; BACK	LDA #35   	; HIM	STA BOAT+0	; AND YOU	JMP ANDDE	; AT LOC 35	BOTM	JSR FACE	; BOAT MAN	LDX #T+71	JSR WINDOW	LDX #T+72	JSR WINDOW	LDA #2	JMP YNCONTQ	JSR FACE	LDA STAKE	LSR A	LSR A	LSR A	LSR A	ORA #"0"	STA STEXB3+70	LDA STAKE	AND #15	ORA #"0"	STA STEXB3+71	LDX #T+47	; DRINK	JSR WINDOW	; CONTEST	LDX #T+48	JSR WINDOW	LDA #1YN	STA YESB	STA NOB	LDA #11	STA LIST+0	LDA #12	STA LIST+1	LDA #255	STA LIST+2	LDA #0	STA LISTO	RTSGIRL	JSR FACE	LDX #T+83	JSR WINDOW		LDA #239	STA REYA	LDA #255	STA MAIN5	JMP DEMM	; ************ 33 **************USE	LDA #12	JMP SLAROUS	JSR FACE	LDY WHATP	LDX OBJ1,Y 	JSR WINDOW	LDA WHATO	STA CHOS+1	LDA WHATS	; SCROLL	JSR LOOSE	LDA #13		; US2	STA DOWHAT	LDX #T+59	; WHAT WITH	JMP WINDOWUS2	LDY WHATP		LDX OBJ1,Y	JSR WINDOW	LDY #0NILIST	LDA FIRSTO,Y	CMP #255	; END OF LIST	BEQ NOEFFCHOS	CMP #255	; FIRST OBJECT	BNE NEXOP	LDA SECOND,Y	CMP WHATO	; SECOND OBJECT	BEQ SPECNEXOP	INY	BNE NILISTNOEFF	LDX #T+19	; NO EFFECT	JSR WINDOW	JMP OKSPEC	LDA ADVTL,Y	STA SOMW+1		LDA ADVTH,Y	STA SOMW+2SOMW	JSR &FFFF	; SMC	JMP OKSP1	LDA OBJ21+1	; DAGGER	CMP #247	; ON	BNE NOEFF	; PHEONIX	LDA ROOM	STA OBJ21+1	LDX #T+60	JMP WINDOWSP2	LDA #32		; CANDLE	STA OBJ14+3	; LIT	LDX #T+64  	; CANDLE LIT	JMP WINDOW	SP3	LDX #T+66	JSR WINDOW	LDA #239	; DESTROY	STA OBJ39+1	; EARTH2	LDA #99		; HOLE2	STA OBJ40+1	; APPEARSNORT	RTSSP4	LDX #T+67	JSR WINDOW	LDA #239	; DESTROY	STA OBJ40+1	; HOLE2	LDA #99		; EARTH2	STA OBJ39+1	; APPEARS	LDA OBJ7+1	; IF RINGD	CMP #243	; IN HOLE	BNE NORT	; DEMON	LDA #239	; DESTROY	STA OBJ7+1	; RINGD	LDA #255	STA MAIN2	LDA ROOM	STA DEMON	LDX #T+69	; TREMMOR	JSR WINDOW		JMP DEMMSP5	LDX #T+75	; DOOR NOW OPEN	JSR WINDOW		LDA #76		; NOW CAN	STA R66+2	; MOVE	RTSSP6	LDX #T+67	JSR WINDOW	LDA #239	; DESTROY	STA OBJ38+1	; HOLE1	LDA #39		; EARTH1	STA OBJ37+1	; APPEARS	RTSSP7	LDX #T+66	JSR WINDOW	LDA #239	; DESTROY	STA OBJ37+1	; EARTH1	LDA #39		; HOLE1	STA OBJ38+1	; APPEARS 	RTS	SP8	LDA MAIN5	BNE GOTN	LDX #T+80		JMP WINDOWGOTN	LDX #T+81	JSR WINDOW	LDA #239	STA OBJ48+1	LDA ROOM	STA SLOUGH	; SLOUGTH LIN	JMP DEMM	; OBJECT COMPARESFIRSTO	DB 31,32,10,10,18,10,10,13,255SECOND	DB 20,14,39,40,44,38,37,48ADVTL	DB >SP1,>SP2,>SP3,>SP4	DB >SP5,>SP6,>SP7,>SP8ADVTH	DB <SP1,<SP2,<SP3,<SP4		DB <SP5,<SP6,<SP7,<SP8	; ************ 34 **************REST	JSR FACE	LDX #T+17	JSR WINDOW	LDX #T+18	JSR WINDOW	LDA #%10000000	; IN CASE YOU	STA MOVEICON	; WERE DRUNK	LDA #0		; NOT DRUNK	STA INTOX	; NOW	LDA #&30	; ADD TO WARP	JSR ADDW	LDA #&10ADT	STA ADDT+1	LDA CIA2+11	; STOP TIMER	SED	LDA CIA2+10	CLCADDT	ADC #&10	STA CIA2+10	LDA CIA2+11	ADC #0	STA CIA2+11	CLD	STA CIA2+8	; START	RTS	; ************ 35 **************DRINKA	LDA DRUNK	CLC	ADC #1	STA DRUNK	LDA INTOX	CLC	ADC #1	STA INTOX	CMP #16	BCC NOPI	LDA #%11000000	STA MOVEICONNOPI	LDA DRUNK	CMP #8		BCS WIN	RTSWIN	LDX #T+52	JSR WINDOW	JSR WON	LDA STAKE	SED	CLC	ADC #1	STA STAKE	CLD	LDA ELAPSE	; VILLAGER	SEC	SBC #20		; GETS BETTER	STA ELAPSE	JSR CLEARB	JSR FADEO	JMP OOC	; ************ 36 **************GIVEUP	JSR CLEARB	JSR FADEO	JSR FACE	LDX #T+53	JSR WINDOW	LDX #T+54	JSR WINDOW	JSR LOSTOOC	LDA #0	STA DRINK	; OUT OF CONTEST	STA YESB	; NORMAL	STA NOB	JMP OKWON	SED	LDA MONEY	CLC	ADC STAKE	STA MONEY	CLD	RTSLOST	SED	LDA MONEY	SEC	SBC STAKE	STA MONEY	CLD	RTS	; ************ 37 **************AROUND	JSR FACE	LDA #126	STA OBJ25+1	LDY #10	LDA (MAPD),Y		TAX	JSR WINDOW	LDA ENEMYT+2	BEQ SWRB	JSR HOMV	; HOW MANY VILL.	JSR WINDOWSWRB	LDA ROOM	JSR DAY	LDA ROOM	STA OBJ25+1	RTS	; ************ 38 **************LOOKIT	LDA #14	JMP SLAROLOK	JSR FACE		LDY WHATO	LDA CONT-1,Y	; IF 0 THEN NOT	BEQ PLB		; A CONTAINER	STA TEMPM	; IF CAN BE	LDY WHATP	; OPENED THEN	LDA OBJ1+3,Y	; CHECK IF SO	STA TEMPM+1		AND #%00001000	; CAN NOT	BNE PLB		; LOOK INTO	LDA TEMPM+1	AND #%00000001	; OPENABLE	BEQ MAY		; NO	LDA TEMPM+1	AND #%00000010	BNE MAY		; AND IS OPEN	LDX OBJ1,Y	; SHOW OBJECT	JSR WINDOW	LDX #T+9	JSR WINDOW	; OBJECT CLOSED	JMP OKMAY	LDA TEMPM	; DISPLAYDAY	JSR SHOWC	; ALL IN CONT.	LDA WHATS	BNE NAS	LDX #T+14	; THERES NOTHING	JSR WINDOWNAS	JMP OKPLB	JMP SLB		; SILLY	; ************ 39 **************LOOKON	LDA #15	JMP SLAROLOKN	JSR FACE		LDY WHATO	LDA CONT-1,Y	; IF 0 THEN NOT	BEQ PLB		; A CONTAINER	STA TEMPM	; IF CAN BE	LDY WHATP	; OPENED THEN	LDA OBJ1+3,Y	; CHECK IF SO	STA TEMPM+1		AND #%00001000	; LOOKABLE	BEQ PLB		; NO	LDX OBJ1,Y	; SHOW OBJECT	JSR WINDOW	LDA TEMPM	; DISPLAY	JSR SHOWC	; ALL IN CONT.	LDA WHATS	BEQ NAS2	RTSNAS2	LDX #T+14	; NOTHING THERE	JMP WINDOW	; ************ 40 **************WRP	JSR FACE	JSR CLEARB	JSR FADEO	LDX #21	JSR WINDOW	LDX #T+42	JSR WINDOWKILTH	LDX #T+43	JSR WINDOW	LDA ROOM	; BRING BACK	STA OBJ25+1	; UKKO BOTTLER	LDA #5		; -5 FROM WARP	JSR SUBW	LDA #239	; WARPEDKNOCK	STA WHATD+1	LDX VILOFFWHATD	LDA #239	; KILL THEM	STA ENEMYS,X	; ALL OFF	LDA #0		; BACK TO	STA FIGHT	; NORMAL	STA ENEMYT+2	; NO ONE	JSR PTEXT	; MESSAGE	LDA MUSIC	BMI NOTUN2	LDA #0	JSR &5700NOTUN2	JMP OK	; ************ 41 **************	KIK	JSR RAND	AND #3	CLC	ADC #1	STA MIKE1	LSR A	LSR A	LSR A	LSR A	ORA #"0"	STA PLEB2+6	LDA MIKE1	AND #&F	ORA #"0"	STA PLEB2+7	LDA MIKE1	JSR MINW	LDX #>K3	LDY #<K3	JSR WHATIW	JMP UPDATE	; ************ 42 **************DUK	LDA ATYPE	CMP #3	BEQ LUCK	RTS	; ************ 43 **************PNCH	LDA #1	JSR MINW	LDX #>K2	LDY #<K2	JSR WHATIW	JMP UPDATE	; ************ 44 **************SWN	LDX #>K4	LDY #<K4	LDA #2	JMP TKOF 	; ************ 45 **************LEP	LDA AXE	BMI NOAXE	LDA #46	JMP GAINNOAXE	RTS	; ************ 46 **************BEHD	LDX #>K7	LDY #<K7	LDA #8	JMP TKOF	; ************ 47 **************DEFN	LDA ATYPE	CMP #1	BEQ LUCK	RTS	; ************ 48 **************DODG	LDA ATYPE	CMP #2	BEQ LUCK	RTSLUCK	LDX #>S1	LDY #<S1	JSR WHATIW	LDA #0	STA EATTACK	LDA #4	STA TIMER	RTS	; ************ 49 **************THRO	LDA #255	STA AXE	LDA #49	JSR LOOSE	LDA #50	JSR GAIN	LDX #>K1	LDY #<K1	LDA #5TKOF	STA ADT2+1	JSR RAND	AND #7	SED	CLCADT2	ADC #5	STA MIKE1	CLD	LSR A	LSR A	LSR A	LSR A	ORA #"0"	STA PLEB1+0	STA PLEB3+0	STA PLEB4+8	LDA MIKE1	AND #&F	ORA #"0"	STA PLEB1+1	STA PLEB3+1	STA PLEB4+9	LDA MIKE1	JSR MINW	JSR WHATIW	JMP UPDATE		; ************ 50 **************SHT	JSR RAND	AND #1	BEQ FUOF	LDX #>K5	LDY #<K5	JSR WHATIW	LDA #0	STA AXE	LDA #50	JSR LOOSE	LDA #49	JMP GAINFUOF	LDX #>K6	LDY #<K6	JMP WHATIW	; ******************************MINW	STA MIKE1	SED			LDA ENEMYW	SEC	SBC MIKE1	BEQ KILLED	BMI KILLED	STA ENEMYW	; +1	CLC	ADC #1	STA MIKE1	LDA #&99	STA MIKE1+1WEZ	LDA MIKE1+1	CLC	ADC #1	STA MIKE1+1	LDA MIKE1	SEC	SBC ENEMYT+3	; HOW MANY	STA MIKE1	BPL WEZ	CLD	LDA MIKE1+1	STA ENEMYT+2	RTSKILLED	CLD	PLA	PLA	LDA ENEMYT+5 	JSR ADDW	; ADD TO WARP	LDA ENEMYT+5	JSR ADDM	; ADD TO MAX	JSR CLEARB	JSR FADEO	JSR FACE	JMP KILTH	